    "build": "nest build",
    "build:css": "tailwindcss -i styles/global.css -o public/assets/styles.css",
    "build:vite": "cd frontend && vite build && rm -rf ../dist/public && cp -rf ./public ../dist/",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:front": "cd frontend && vite",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "start:ai-cli": "ts-node ai-editor/src/cli.ts",
    "start:generate-module": "ts-node libs/cli.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
